{
    "name": "californiamountainsnake/laravel-migrations",
    "type": "library",
    "description": "Use different Migration classes for different database connections in Laravel.",
    "license": "MIT",
    "authors": [
        {
            "name": "CaliforniaMountainSnake",
            "email": "CaliforniaMountainSnake1@yandex.ru"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-pdo": "*",
        "illuminate/database": "*"
    },
    "require-dev": {
        "ext-dom": "*",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "CaliforniaMountainSnake\\LaravelMigrations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "\"vendor/bin/phpunit\""
        ]
    }
}
